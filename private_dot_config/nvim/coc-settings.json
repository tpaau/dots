{
	"snippets.ultisnips.pythonPrompt": false,
	"languageserver": {
    	"clangd": {
			"command": "clangd",
			"filetypes": ["c", "cpp", "objc", "objcpp"]
    	},
		"pyright": {
    		"command": "pyright-langserver",
    		"args": ["--stdio"],
    		"filetypes": ["python", "py"]
    	},
		"glsl_analyzer": {
			"command": "glsl_analyzer",
			"filetypes": ["glsl", "frag", "vert", "comp", "gdshader"]
		},
		"omnisharp": {
			"command": "omnisharp",
			"filetypes": ["cs"]
		},
		"bash-language-server": {
			"command": "bash-language-server",
			"args": ["start"],
			"filetypes": ["sh", "bash"],
		},
		"qml": {
			"command": "qmlls",
			"filetypes": ["qml"],
			"rootPatterns": ["*.qml"],
		}
	}
}
